<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.GameController">
    <Canvas fx:id="gameCanvas" width="600" height="800" />
    <Label fx:id="scoreLabel" text="Score: 0" layoutX="10" layoutY="10"/>
    <Button fx:id="startButton" text="Start" layoutX="250" layoutY="750" onAction="#startGame"/>
</AnchorPane>
